{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "es5",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "outDir": "./dist",
        "baseUrl": ".",
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./front",
        "node_modules"
    ]
}